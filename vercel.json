{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "builds": [{
        "src": "./dist/index.js",
        "use": "@vercel/node",
        "config": {
            "includeFiles": [
                "dist/**"
            ]
        }
    }],
    "rewrites": [{
        "source": "/(.*)",
        "destination": "dist/index.js"
    }]
}