{
    "version": 2,
    "builds": [{
        "src": "./dist/index.js",
        "use": "@vercel/node",
        "buildCommand": "npm run build",
        "outputDirectory": "dist",
        "config": {
            "includeFiles": [
                "dist/**"
            ]
        }
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "/"
    }]
}